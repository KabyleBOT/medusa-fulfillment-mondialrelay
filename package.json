{
	"name": "medusa-fulfillment-mondialrelay",
	"version": "0.3.6",
	"description": "An unofficial Medusa plugin for Mondial Relay shipping",
	"author": "khaled Belkouche <khaled.belkouche@assalas.com>",
	"license": "MIT",
	"keywords": [
		"plugin",
		"medusa",
		"ecommerce",
		"typescript",
		"mondial-relay",
		"shipping"
	],
	"files": [
		"dist"
	],
	"scripts": {
		"check-types": "tsc --noEmit",
		"prepublishOnly": "cross-env NODE_ENV=production tsc --build",
		"clean": "cross-env ./node_modules/.bin/rimraf dist",
		"build:server": "cross-env npm run clean && tsc -p tsconfig.server.json",
		"dev": "cross-env npm run build:server && medusa develop",
		"test": "jest --passWithNoTests src",
		"build": "rimraf dist && tsc -p ./tsconfig.server.json",
		"watch": "tsc --watch"
	},
	"dependencies": {
		"soap": "^1.0.0",
		"xml2js": "^0.6.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.3",
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.24.4",
		"@babel/preset-typescript": "^7.21.4",
		"@medusajs/admin": "latest",
		"@medusajs/icons": "^1.0.0",
		"@medusajs/medusa": "^1.20.9",
		"@medusajs/medusa-cli": "^1.3.16",
		"@medusajs/ui": "^1.0.0",
		"@tanstack/react-query": "latest",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.8",
		"axios": "^1.6.8",
		"babel-preset-medusa-package": "^1.1.13",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "16.0.3",
		"eslint": "^6.8.0",
		"express": "^4.17.2",
		"jest": "^27.3.1",
		"medusa-interfaces": "^1.0.0",
		"medusa-react": "^9.0.6",
		"mongoose": "^5.13.14",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.0.7",
		"ts-loader": "^9.2.6",
		"typeorm": "^0.3.16",
		"typescript": "^4.5.2"
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsconfig": "tsconfig.spec.json"
			}
		},
		"moduleNameMapper": {
			"^axios$": "axios/dist/node/axios.cjs"
		},
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/node_modules/"
		],
		"rootDir": "src",
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
		"transform": {
			".ts": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "./coverage",
		"testEnvironment": "node"
	}
}
